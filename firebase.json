{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/api",
        "region": "us-central1",
        "function": "api"
      }
    ]
  },
  "functions": [
    {
      "source": ".",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "runtime": "nodejs18"
    }
  ]
}
